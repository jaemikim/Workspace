package kr.co.himedia.utils;

import jakarta.servlet.jsp.JspWriter;

public class JSFunction {

	// 메이시 알림장을 띄운 후 명시한 URL로 이동함
	public static void alertLocation(String msg, String url, JspWriter out) {
		String script = ""
				        + "<script>"
				        + "       alert('"+ msg +"');"
				        + "       location.href = '" +url "';"
	
						+ "</script>"
	}
}
